    "# `1741_Find Total Time Spent by Each Employee`"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "f7dab801",
   "metadata": {
    "vscode": {
     "languageId": "sql"
    }
   },
   "outputs": [],
   "source": [
    "SELECT \n",
    "    event_day AS day, \n",
    "    emp_id,\n",
    "    SUM(out_time - in_time) AS total_time\n",
    "FROM Employees\n",
    "GROUP BY day, emp_id"
   ]
  }
 ],
