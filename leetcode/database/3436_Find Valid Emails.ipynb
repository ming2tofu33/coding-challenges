   },
   "outputs": [],
   "source": [
    "-- Oracle\n",
    "SELECT \n",
    "    user_id, \n",
    "    email\n",
    "FROM Users\n",
    "WHERE REGEXP_LIKE(email, '^[A-Za-z0-9_]+@[[:alpha:]]+\\.com$')\n",
    "ORDER BY user_id;"
   ]
  }
