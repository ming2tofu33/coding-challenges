    }
   },
   "outputs": [],
   "source": [
    "SELECT\n",
    "    stock_name,\n",
    "    SUM(CASE \n",
    "        WHEN operation = 'Buy' THEN -price\n",
    "        ELSE price END) AS capital_gain_loss\n",
    "FROM Stocks\n",
    "GROUP BY stock_name"
   ]
  }
 ],
