    "# `176_Second Highest Salary`"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "3b253e36",
   "metadata": {
    "vscode": {
     "languageId": "sql"
    }
   },
   "outputs": [],
   "source": [
    "WITH cte AS (\n",
    "    SELECT\n",
    "        salary,\n",
    "        DENSE_RANK() OVER (ORDER BY salary DESC) AS salary_rank\n",
    "    FROM Employee \n",
    ")\n",
    "SELECT \n",
    "    MAX(case when salary_rank=2 then salary else null end) AS SecondHighestSalary \n",
    "FROM cte\n",
    "LIMIT 1"
   ]
  }
 ],
