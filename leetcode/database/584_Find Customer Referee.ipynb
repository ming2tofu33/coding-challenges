    "import pandas as pd\n",
    "\n",
    "def find_customer_referee(customer: pd.DataFrame) -> pd.DataFrame:\n",
    "    mask = customer[\"referee_id\"].isna() | (customer[\"referee_id\"] != 2)\n",
    "    return customer.loc[mask, [\"name\"]]"
   ]
