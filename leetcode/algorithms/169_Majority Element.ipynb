    "# `169_Majority Element`"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "5dde705c",
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def majorityElement(self, nums: list[int]) -> int:\n",
    "        s = set(nums)\n",
    "        \n",
    "        for i in s:\n",
    "            if nums.count(i) >= len(nums) / 2:\n",
    "                return i"
   ]
  },
